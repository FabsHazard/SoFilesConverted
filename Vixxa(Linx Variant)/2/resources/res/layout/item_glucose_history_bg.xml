    <View android:id="@+id/glucose_history_divider" android:background="?attr/dividerLine" android:visibility="gone" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginTop="10dp" android:layout_marginBottom="16dp"/>
</LinearLayout>
